{
    "manifest_version": 2,
    "name": "Beep Boop",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["popup.js"]
        }
      ],
      "permissions": [ 
        "tabs"
     ],
      "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      }
      
    
  }